    "# # Comparing the models using kfold cross validation\n",
    "\n",
    "# In[70]:\n",
    "X=digits.data;\n",
    "\n",
    "y=digits.target;\n",
    "X=pd.DataFrame(X,columns=[\"pixel\"+str(i) for i in range(0, 64)])\n",
    "\n",
    "\n",
    "import pandas as pd\n",
    "\n",
    "X=pd.DataFrame(X,columns=[\"pixel\"+str(i) for i in range(0, 64)])\n",
    "y=pd.DataFrame(y,columns=['Y'])\n",
    "#on Recupere les lignes correspondant ua chiffre 1 et au chiffre 7\n",
    "a=y['Y']==9\n",
    "b=y['Y']==3\n",
    "y=y[a|b]\n",
    "X=X[a|b]\n",
    "X_new=SelectKBest(f_classif).fit_transform(X, y)\n",
    "X_new=pd.DataFrame(X_new,columns=[\"pixel\"+str(i) for i in range(0, 10)])\n",
    "X=X_new\n",
    "\n",
    "kfold = model_selection.KFold(n_splits=20)\n",
    "model_kfold = RandomForestClassifier(max_depth=5, random_state=0,max_features= 'log2',n_estimators=50)\n",
    "acc_forest = model_selection.cross_val_score(model_kfold, X, y, cv=kfold,scoring= 'accuracy')\n",

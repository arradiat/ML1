    "\n",
    "kfold = model_selection.KFold(n_splits=20)\n",
    "model_kfold = AdaBoostClassifier(n_estimators=100, random_state=0,learning_rate=0.1)\n",
    "acc_adaboost = model_selection.cross_val_score(model_kfold, X, y, cv=kfold,scoring= 'accuracy')\n",
    "#print(\"Accuracy: %.2f%%\" % (results_kfold.mean()*100.0)) \n",
    "\n",
    "\n",
    "# In[74]:\n",
    "\n",
    "\n",
    "kfold = model_selection.KFold(n_splits=20)\n",
    "model_kfold = KNeighborsClassifier(n_neighbors=7)\n",
    "acc_KNN = model_selection.cross_val_score(model_kfold, X, y, cv=kfold,scoring= 'accuracy')\n",
    "#print(\"Accuracy: %.2f%%\" % (results_kfold.mean()*100.0)) \n",
    "\n",

    "### choice of  the number of neighbors  k \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.neighbors import KNeighborsClassifier"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The value of k for which we have the maximum accuracy on the testing set is 7\n"
     ]
    },
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "k_range=np.arange(15)+1 #donc entre 1 et 15\n",
    "scores_list=[]\n",
    "for k in k_range:\n",
    "    knn=KNeighborsClassifier(n_neighbors=k)\n",
    "    knn.fit(X_train, Y_train)\n",
    "    y_pred=knn.predict(X_test)\n",
    "    scores_list.append(sklearn.metrics.accuracy_score(Y_test, y_pred))\n",
    "\n",
    "\n",
    "plt.plot(k_range, scores_list)\n",
    "plt.xlabel(\"value of k\")\n",
    "plt.ylabel(\"Testing accuracy\")\n",
    "\n",
    "print(\"The value of k for which we have the maximum accuracy on the testing set is\",k_range[np.array(scores_list).argmax()])\n"
   ]
  },

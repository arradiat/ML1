    "# ## LDA\n",
    "\n",
    "# In[30]:\n",
    "\n",
    "\n",
    "lda=LinearDiscriminantAnalysis(solver=\"svd\",store_covariance=True)\n",
    "lda.fit(X_train,Y_train)\n",
    "Y_pred=lda.fit(X_train,Y_train).predict(X_test)\n",
    "print(\"precision:\",metrics.accuracy_score(Y_test, Y_pred))\n",
    "\n",
    "\n",
    "# In[34]:\n",
    "\n",
    "\n",
    "print(classification_report(Y_test,y_pred))\n",
    "from sklearn.metrics import plot_confusion_matrix\n",
    "disp = metrics.plot_confusion_matrix(lda, X_test, Y_test)\n",
    "disp.figure_.suptitle(\"Confusion Matrix\")\n",
    "print(\"Confusion matrix:\\n%s\" % disp.confusion_matrix)\n",
    "\n",
    "\n",
    "# the naive bayes is really good to good kinda anoying sah \n",
    "# ### Faisons un Kfold pour voir l'accuracy en moyenne \n",
    "\n",
    "# In[35]:\n",
    "\n",
    "\n",
    "\n",
    "kfold = model_selection.KFold(n_splits=20)\n",
    "model_kfold =  LinearDiscriminantAnalysis()\n",
    "acc_LDA = model_selection.cross_val_score(model_kfold, X, y, cv=kfold)\n",
    "print(\"accuracy: %.2f%%\" % (acc_LDA.mean()*100.0))\n",
    "\n",
    "\n",
    "# ## logistic regression\n",
    "\n",
    "# In[37]:\n",
    "\n",
    "\n",
    "from sklearn import linear_model\n",
    "logreg=linear_model.LogisticRegression(C=1e5,solver='lbfgs')\n",
    "logreg.fit(X_train, Y_train)\n",
    "y_pred=logreg.predict(X_test)\n",
    "reclr=sklearn.metrics.recall_score(Y_test, y_pred,pos_label=9)\n",
    "acclr=sklearn.metrics.accuracy_score(Y_test, y_pred)\n",
    "preclr=sklearn.metrics.precision_score(Y_test, y_pred,pos_label=9)\n",
    "f1lr=sklearn.metrics.f1_score(Y_test, y_pred,pos_label=9)\n",
    "\n",
    "\n",
    "# In[39]:\n",
    "\n",
    "\n",
    "print(classification_report(Y_test,y_pred))\n",
    "from sklearn.metrics import plot_confusion_matrix\n",
    "disp = metrics.plot_confusion_matrix(logreg, X_test, Y_test)\n",
    "disp.figure_.suptitle(\"Confusion Matrix\")\n",
    "print(\"Confusion matrix:\\n%s\" % disp.confusion_matrix)\n",
    "\n",
    "\n",
    "# ## KNN\n",
    "\n",
    "# ### choice of  the number of neighbors  k \n",
    "# \n",
    "\n",
    "# In[41]:\n",
    "\n",
    "\n",
    "from sklearn.neighbors import KNeighborsClassifier\n",
    "\n",
    "\n",
    "# In[42]:\n",
    "\n",
    "\n",
    "k_range=np.arange(15)+1 #donc entre 1 et 15\n",
    "scores_list=[]\n",
    "for k in k_range:\n",
    "    knn=KNeighborsClassifier(n_neighbors=k)\n",
    "    knn.fit(X_train, Y_train)\n",
    "    y_pred=knn.predict(X_test)\n",
    "    scores_list.append(sklearn.metrics.accuracy_score(Y_test, y_pred))\n",
    "\n",
    "\n",
    "plt.plot(k_range, scores_list)\n",
    "plt.xlabel(\"value of k\")\n",
    "plt.ylabel(\"Testing accuracy\")\n",
    "\n",
    "\n",
    "\n",
    "# In[45]:\n",
    "\n",
    "\n",
    "print(\"The value of k for which we have the maximum accuracy\",k_range[np.array(scores_list).argmax()])\n",
    "\n",
    "\n",
